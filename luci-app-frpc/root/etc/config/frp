
config frp 'common'
	option log_max_days '3'
	option login_fail_exit '0'
	option enable_cpool '0'
	option time '40'
	option tcp_mux '1'
	option enabled '0'
	option vhost_http_port '80'
	option vhost_https_port '443'
	option server_port '7000'
	option server_addr 'freenat.club'
	option token 'frp888 '
	option log_level 'info'
	option enable_http_proxy '0'
	option protocol 'tcp'
	option ver '0.25.1'
	option url '2'
	option bin_path '/tmp/etc/frp/frpc'

config proxy
	option enable '0'
	option type 'http'
	option domain_type 'custom_domains'
	option custom_domains 'openwrt'
	option local_ip '127.0.0.1'
	option local_port '80'
	option use_encryption '1'
	option use_compression '1'
	option remark 'http_80'